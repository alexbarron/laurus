<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_with model: @developer_app do |f| %>

            <%= render 'shared/error_messages', object: f.object %>

            <%= f.label "App Name" %>
            <%= f.text_field :name, class: 'form-control' %>

            <%= f.submit "Submit", class: "btn btn-primary" %>

        <% end %>
    </div>
</div>